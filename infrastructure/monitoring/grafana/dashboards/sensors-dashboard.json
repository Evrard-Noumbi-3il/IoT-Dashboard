{
  "title": "IoT Sensors Dashboard",
  "timezone": "browser",
  "panels": [
    {
      "type": "timeseries",
      "title": "Temperature (°C)",
      "targets": [{ "expr": "iot_temperature" }],
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Humidity (%)",
      "targets": [{ "expr": "iot_humidity" }],
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "CO2 (ppm)",
      "targets": [{ "expr": "iot_co2" }],
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Light (lux)",
      "targets": [{ "expr": "iot_light" }],
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Noise (dB)",
      "targets": [{ "expr": "iot_noise" }],
      "gridPos": { "x": 0, "y": 16, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Pressure (Pa)",
      "targets": [{ "expr": "iot_pressure" }],
      "gridPos": { "x": 12, "y": 16, "w": 12, "h": 8 }
